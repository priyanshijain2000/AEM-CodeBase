<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Icon Button"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <maincontent
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Main Content"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <cards
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                composite="{Boolean}true"
                                                fieldLabel="Card List"
                                                required="{Boolean}false">
                                                <field
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                                    name="./cards">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <icon
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                            fieldLabel="Card Icon"
                                                            name="./icon"
                                                            rootPath="/content/dam/ets-org"/>
                                                        <alttext1
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldDescription="Textual alternative of the meaning or function of the image, for visually impaired readers."
                                                            fieldLabel="Alternative text for accessibility"
                                                            name="./altText1"
                                                            required="{Boolean}true"/>
                                                        <hovericon
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                            fieldLabel="Hover Icon"
                                                            name="./hovericon"
                                                            rootPath="/content/dam/ets-org"/>
                                                        <alttext2
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldDescription="Textual alternative of the meaning or function of the image, for visually impaired readers."
                                                            fieldLabel="Alternative text for accessibility"
                                                            name="./altText2"
                                                            required="{Boolean}true"/>
                                                        <title
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldLabel="Text"
                                                            name="./text"/>
                                                        <link
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                            fieldLabel="Link"
                                                            name="./link"
                                                            rootPath="/content/ets-org"/>
														<linkarialabel
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldLabel="Link Aria Label"
                                                            name="./linkAriaLabel"/>
                                                        <target
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                            name="./newTab"
                                                            text="Link target to new window"
                                                            uncheckedValue="{Boolean}false"
                                                            value="{Boolean}true"/>
                                                        <spa
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                            name="./spa"
                                                            text="SPA"
                                                            uncheckedValue="{Boolean}false"
                                                            value="{Boolean}true"/>
                                                    </items>
                                                </field>
                                            </cards>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </maincontent>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
