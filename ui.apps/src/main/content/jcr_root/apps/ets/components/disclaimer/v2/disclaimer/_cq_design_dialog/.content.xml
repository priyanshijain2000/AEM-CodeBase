<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        granite:class="cmp-container__editor"
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <documentation
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Documentation"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <container
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                 <notes
                                        granite:class="coral-Form-fieldlabel"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/text"
                                        text="Note: This is using the Core Component - Text Component."/>
                                    <styleoptions
                                        granite:class="coral-Form-fieldlabel"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/text"
                                        text="Style Options"/>
                                    <firstoption
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/well">
                                        <items jcr:primaryType="nt:unstructured">
                                            <layout
                                                granite:class="coral-Form-fieldlabel"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/text"
                                                text="Styles"/>
                                            <firstinner
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/well">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <warning
                                                        granite:class="coral-Form-fieldlabel"
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/text"
                                                        text="cmp-disclaimer--warning"/>
                                                    <info
                                                        granite:class="coral-Form-fieldlabel"
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/text"
                                                        text="cmp-disclaimer--info"/>
                                                    <shortnote
                                                        granite:class="coral-Form-fieldlabel"
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/text"
                                                        text="cmp-disclaimer--warning-short-note"/>
                                                    <error
                                                        granite:class="coral-Form-fieldlabel"
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/text"
                                                        text="cmp-disclaimer--error"/>
                                                    <error
                                                        granite:class="coral-Form-fieldlabel"
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/text"
                                                        text="cmp-disclaimer--disabled"/>
                                                     </items>
                                            </firstinner>
                                        </items>
                                    </firstoption>
                                </items>
                            </container>
                        </items>
                    </documentation>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
