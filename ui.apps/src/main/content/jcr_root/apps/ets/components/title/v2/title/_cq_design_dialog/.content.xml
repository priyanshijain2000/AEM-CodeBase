<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        granite:class="cmp-container__editor"
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <documentation
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Documentation"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <container
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                 <note
                                        granite:class="coral-Form-fieldlabel"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/text"
                                        text="Note: This is an OOTB Adobe Core Component. Authors: For spacing, please put this component within a container. Style names should be seperate from TAG names - so a tag can be H1, but the style can be Heading-3, etc. Tags should be determined by WCAG."/>
                                    <styleoptions
                                        granite:class="coral-Form-fieldlabel"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/text"
                                        text="Style Options"/>
                                    <firstoption
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/well">
                                        <items jcr:primaryType="nt:unstructured">
                                            <layout
                                                granite:class="coral-Form-fieldlabel"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/text"
                                                text="Title Fonts"/>
                                            <firstinner
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/well">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <h1
                                                        granite:class="coral-Form-fieldlabel"
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/text"
                                                        text="cmp-title--heading-1"/>
                                                    <h2
                                                        granite:class="coral-Form-fieldlabel"
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/text"
                                                        text="cmp-title--heading-2"/>
                                                    <h3
                                                        granite:class="coral-Form-fieldlabel"
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/text"
                                                        text="cmp-title--heading-3"/>
                                                    <h4
                                                        granite:class="coral-Form-fieldlabel"
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/text"
                                                        text="cmp-title--heading-4"/>
                                                    <h5
                                                        granite:class="coral-Form-fieldlabel"
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/text"
                                                        text="cmp-title--heading-5"/>
                                                    <h6
                                                        granite:class="coral-Form-fieldlabel"
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/text"
                                                        text="cmp-title--heading-6"/>
                                                     </items>
                                            </firstinner>
                                        </items>
                                    </firstoption>
                                    <secondoption
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/well">
                                        <items jcr:primaryType="nt:unstructured">
                                            <layout
                                                granite:class="coral-Form-fieldlabel"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/text"
                                                text="Colors"/>
                                                 <secondinner
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/well">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <black
                                                            granite:class="coral-Form-fieldlabel"
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/text"
                                                            text="cmp-title--black"/>
                                                        <white
                                                            granite:class="coral-Form-fieldlabel"
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/text"
                                                            text="cmp-title--white"/>
                                                        <blue
                                                            granite:class="coral-Form-fieldlabel"
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/text"
                                                            text="cmp-title--blue"/>
                                                        <theme
                                                            granite:class="coral-Form-fieldlabel"
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/text"
                                                            text="cmp-title--brandtheme"/>
                                                    </items>
                                                </secondinner>
                                        </items>
                                    </secondoption>
                                    <thirdoption
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/well">
                                        <items jcr:primaryType="nt:unstructured">
                                            <layout
                                                granite:class="coral-Form-fieldlabel"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/text"
                                                text="Alignment"/>
                                                <thirdinner
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/well">
                                                    <items jcr:primaryType="nt:unstructured">
                                                    <right
                                                        granite:class="coral-Form-fieldlabel"
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/text"
                                                        text="cmp-title--right"/>
                                                    <center
                                                        granite:class="coral-Form-fieldlabel"
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/text"
                                                        text="cmp-title--center"/>
                                                    <centertd 
                                                        granite:class="coral-Form-fieldlabel"
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/text"
                                                        text="cmp-title--centertd (Tablet/Desktop only)"/>
                                                    <left
                                                        granite:class="coral-Form-fieldlabel"
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/text"
                                                        text="cmp-title--left"/>
                                                        </items>
                                                </thirdinner>
                                        </items>
                                    </thirdoption>
                                    <fourthoption
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/well">
                                        <items jcr:primaryType="nt:unstructured">
                                            <layout
                                                granite:class="coral-Form-fieldlabel"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/text"
                                                text="Padding"/>
                                                <fourthinner
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/well">
                                                    <items jcr:primaryType="nt:unstructured">
                                                       <topxs
                                                            granite:class="coral-Form-fieldlabel"
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/text"
                                                            text="cmp-title--padding-top-xs"/>
                                                        <topsm
                                                            granite:class="coral-Form-fieldlabel"
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/text"
                                                            text="cmp-title--padding-top-sm"/>
                                                        <topmed
                                                            granite:class="coral-Form-fieldlabel"
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/text"
                                                            text="cmp-title--padding-top-med"/>
                                                        <toplg
                                                            granite:class="coral-Form-fieldlabel"
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/text"
                                                            text="cmp-title--padding-top-lg"/>
                                                        <topxl
                                                            granite:class="coral-Form-fieldlabel"
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/text"
                                                            text="cmp-title--padding-top-xl"/>
                                                        <bxs
                                                            granite:class="coral-Form-fieldlabel"
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/text"
                                                            text="cmp-title--padding-bottom-xs"/>
                                                        <bsm
                                                            granite:class="coral-Form-fieldlabel"
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/text"
                                                            text="cmp-title--padding-bottom-sm"/>
                                                        <bmed
                                                            granite:class="coral-Form-fieldlabel"
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/text"
                                                            text="cmp-title--padding-bottom-med"/>
                                                        <blg
                                                            granite:class="coral-Form-fieldlabel"
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/text"
                                                            text="cmp-title--padding-bottom-lg"/>
                                                        <bxl
                                                            granite:class="coral-Form-fieldlabel"
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/text"
                                                            text="cmp-title--padding-bottom-xl"/>
                                                        </items>
                                                </fourthinner>
                                        </items>
                                    </fourthoption>
                                </items>
                            </container>
                        </items>
                    </documentation>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
