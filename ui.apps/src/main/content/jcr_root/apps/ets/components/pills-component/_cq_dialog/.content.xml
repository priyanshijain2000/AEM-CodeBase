<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Parallax image"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <logo
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Configuration"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <pagetype
                                                granite:class="cq-dialog-dropdown-showhide"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                emptyText="Choose the Page Type"
                                                fieldLabel="Page Type"
                                                name="./pageType">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <option1
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Generic Page"
                                                        value="generic"/>
                                                    <option2
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Praxis Page"
                                                        value="praxis"/>
                                                    <option3
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Toefl Page"
                                                        value="toefl"/>
                                                    <option4
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Gre Page"
                                                        value="gre"/>
                                                    <option5
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Toeic Page"
                                                        value="toeic"/>
                                                    <option6
                                                        jcr:primaryType="nt:unstructured"
                                                        text="R&amp;D Page"
                                                        value="rnd"/>
                                                    <option7
                                                        jcr:primaryType="nt:unstructured"
                                                        text="PSQ Page"
                                                        value="psq"/>
                                                    <option8
                                                        jcr:primaryType="nt:unstructured"
                                                        text="EAS Page"
                                                        value="eas"/>
                                                </items>
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    cq-dialog-dropdown-showhide-target=" .path-showhide-target"/>
                                            </pagetype>
                                            <generic
                                                granite:class="path-showhide-target"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <generic-pill
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                        emptyText="Choose a pill"
                                                        fieldLabel="Generic pill"
                                                        name="./genericPill">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <option1
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Article Pill"
                                                                value="home/article-pills.html"/>
                                                            <option2
                                                                jcr:primaryType="nt:unstructured"
                                                                text="InfoCard Pill"
                                                                value="about/infocard-pills.html"/>
                                                            <option3
                                                                jcr:primaryType="nt:unstructured"
                                                                text="SPA Pill"
                                                                value="home/spa-pills.html"/>
                                                            <option4
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Error Pill"
                                                                value="about/error-pills.html"/>
                                                            <option5
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Product Family Pill"
                                                                value="about/productfamily-pills-toefl.html"/>
                                                            <option6
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Social Pill"
                                                                value="about/social-pills.html"/>
                                                            <option7
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Footer Pill"
                                                                value="about/about-footer-pills.html"/>
                                                        </items>
                                                    </generic-pill>
                                                </items>
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    showhidetargetvalue="generic"/>
                                            </generic>
                                            <praxis
                                                granite:class="path-showhide-target"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <praxis-pill
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                        emptyText="Choose a pill"
                                                        fieldLabel="Praxis pill"
                                                        name="./praxisPill">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <option1
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Top Pill"
                                                                value="praxis/pills1.html"/>
                                                            <option2
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Top Pill Sec Nav"
                                                                value="praxis/pills1-secnav.html"/>
                                                            <option3
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Footer Pill"
                                                                value="praxis/pills4.html"/>
                                                        </items>
                                                    </praxis-pill>
                                                </items>
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    showhidetargetvalue="praxis"/>
                                            </praxis>
                                            <toefl
                                                granite:class="path-showhide-target"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <toefl-pill
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                        emptyText="Choose a pill"
                                                        fieldLabel="Toefl pill"
                                                        name="./toeflPill">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <option1
                                                                jcr:primaryType="nt:unstructured"
                                                                text="SPA Pill(Single)"
                                                                value="toefl/spa-pill-toefl-right.html"/>
                                                            <option2
                                                                jcr:primaryType="nt:unstructured"
                                                                text="SPA Pill(Double)"
                                                                value="toefl/spa-pill-toefl-both.html"/>
                                                            <option3
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Essentials Pill"
                                                                value="toefl/essentials-pill-toefl.html"/>
                                                        </items>
                                                    </toefl-pill>
                                                </items>
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    showhidetargetvalue="toefl"/>
                                            </toefl>
                                            <gre
                                                granite:class="path-showhide-target"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <gre-pill
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                        emptyText="Choose a pill"
                                                        fieldLabel="Gre pill"
                                                        name="./grePill">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <option1
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Left Pill"
                                                                value="gre/pillsleft.html"/>
                                                            <option2
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Right Pill"
                                                                value="gre/pillsright.html"/>
                                                        </items>
                                                    </gre-pill>
                                                </items>
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    showhidetargetvalue="gre"/>
                                            </gre>
                                            <toeic
                                                granite:class="path-showhide-target"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <toeic-pill
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                            emptyText="Choose a pill"
                                                            fieldLabel="Toeic pill"
                                                            name="./toeicPill">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <option1
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Left Pill"
                                                                value="toeic/pillsleft.html"/>
                                                            <option2
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Right Pill"
                                                                value="toeic/pillsright.html"/>
                                                        </items>
                                                    </toeic-pill>
                                                </items>
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    showhidetargetvalue="toeic"/>
                                            </toeic>
                                            <rnd
                                                granite:class="path-showhide-target"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <rnd-pill
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                        emptyText="Choose a pill"
                                                        fieldLabel="R&amp;D pill"
                                                        name="./rndPill">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <option1
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Banner Pill"
                                                                value="rnd/rnd-banner.html"/>
                                                            <option2
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Footer Pill"
                                                                value="rnd/rndfooter-pills.html"/>
                                                        </items>
                                                    </rnd-pill>
                                                </items>
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    showhidetargetvalue="rnd"/>
                                            </rnd>
                                            <psq
                                                granite:class="path-showhide-target"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <psq-pill
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                        emptyText="Choose a pill"
                                                        fieldLabel="PSQ Pill"
                                                        name="./psqPill">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <option1
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Left Pill"
                                                                value="psq/pillsleft.html"/>
                                                        </items>
                                                    </psq-pill>
                                                </items>
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    showhidetargetvalue="psq"/>
                                            </psq>
                                            <eas
                                                granite:class="path-showhide-target"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <eas-pill
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                        emptyText="Choose a pill"
                                                        fieldLabel="EAS Pills"
                                                        name="./easPill">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <option1
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Info card Pills"
                                                                value="eas/infocard-pills.html"/>
                                                        </items>
                                                    </eas-pill>
                                                </items>
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    showhidetargetvalue="eas"/>
                                            </eas>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </logo>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
